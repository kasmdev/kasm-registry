      "assign": {
        "cmd": "bash -c '/dockerstartup/custom_startup.sh --assign --url \"$KASM_URL\"'"
      }
    },
    "architecture": [
      "amd64",
      "arm64"
    ],
    "compatibility": [
      "1.13.x",
      "1.14.x"
    ],
    "uncompressed_size_mb": 7540
  }

{
    "friendly_name": "android studio",
    "image_src": "android-studio.png",
    "description": "testing",
    "name": "budtmo/docker-android:emulator_14.0",
    "cores": 2,
    "memory": 5000,
    "gpu_count": 0,
    "cpu_allocation_method": "Inherit",
    "docker_registry": "https://index.docker.io/v1/",
    "categories": [
      "Development"
    ],
    "require_gpu": false,
    "enabled": true,
    "image_type": "Container",
    "run_config": {
      "hostname": "kasm"
    },
    "exec_config": {
      "go": {
        "cmd": "bash -c '/dockerstartup/custom_startup.sh --go --url \"$KASM_URL\"'"
